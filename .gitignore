# ===========================================
# GYMapp Flutter - Comprehensive .gitignore
# ===========================================

# ============ SENSITIVE FILES - NEVER COMMIT ============

# Environment and secret files
.env
.env.*
*.env
secrets.dart
**/secrets/

# Local properties (contains SDK paths and local config)
local.properties
**/local.properties

# Firebase service account keys and configs
*-service-account.json
service-account*.json
google-services.json
**/google-services.json
GoogleService-Info.plist
**/GoogleService-Info.plist

# API keys and credentials
*.keystore
*.jks
key.properties
credentials.json
**/credentials/

# Cloud function secrets
functions/.env
functions/.env.*

# Firebase emulator data
.firebase/
firebase-debug.log
firestore-debug.log
ui-debug.log
database-debug.log
pubsub-debug.log

# Backup folder with sensitive data
android_backup/

# Environment path file
env_path.txt

# ============ FLUTTER/DART ============

# Flutter generated files
**/doc/api/
**/ios/Flutter/.last_build_id
.dart_tool/
.flutter-plugins
.flutter-plugins-dependencies
.pub-cache/
.pub/
/build/
/coverage/

# Symbolication related
app.*.symbols

# Obfuscation related
app.*.map.json

# Generated Dart files
*.g.dart
*.freezed.dart
*.mocks.dart

# ============ IDE & EDITORS ============

# IntelliJ related
*.iml
*.ipr
*.iws
.idea/

# VS Code
.vscode/

# Android Studio
*.iml

# ============ OS GENERATED ============

# macOS
.DS_Store
.AppleDouble
.LSOverride
._*

# Windows
Thumbs.db
ehthumbs.db
Desktop.ini

# Linux
*~

# ============ BUILD ARTIFACTS ============

# Android Studio build artifacts
/android/app/debug
/android/app/profile
/android/app/release
/android/.gradle/
/android/captures/
/android/app/build/

# iOS build artifacts
/ios/Pods/
/ios/.symlinks/
/ios/Flutter/Flutter.framework
/ios/Flutter/Flutter.podspec
/ios/Flutter/engine
/ios/Flutter/App.framework

# Miscellaneous
*.class
*.log
*.pyc
*.swp
.atom/
.build/
.buildlog/
.history
.svn/
.swiftpm/
migrate_working_dir/

# Backup files
*.backup
*.bak

# ============ TEST & COVERAGE ============

coverage/
.test_coverage.dart
.dart_tool/

run_app.bat

build_apk.bat
